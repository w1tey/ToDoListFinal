<Application x:Class="ToDoListFinal.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:ToDoListFinal"
             xmlns:ViewModel="clr-namespace:ToDoListFinal.ViewModel"
             xmlns:View="clr-namespace:ToDoListFinal.View"
             StartupUri="/View/MainWindow.xaml">
    <Application.Resources>
        <DataTemplate DataType="{x:Type ViewModel:TaskViewModel}">
            <View:MainWindow></View:MainWindow>
        </DataTemplate>
    </Application.Resources>
</Application>
